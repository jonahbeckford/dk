  "schema_version": { "major": 1, "minor": 0 },
  "forms": [
    {
      "id": "CommonsBase_Std.Coreutils.Exe@0.2.2",
      "precommands": {
        "private": [
          // need bin/<architecture>/7z.exe (7zr does not extract tar files)
          "get-object CommonsBase_Std.S7z.Windows7zExe@25.1.0 -s File.Windows_x86 -d bin/File.Windows_x86",
          // need macOS and Linux tar files. since same destination directory need [install-object]
          "install-object CommonsBase_Std.Coreutils.Tar@0.2.2 -s File.Darwin_arm64 -d tarballs",
          "install-object CommonsBase_Std.Coreutils.Tar@0.2.2 -s File.Darwin_x86_64 -d tarballs",
          "install-object CommonsBase_Std.Coreutils.Tar@0.2.2 -s File.Linux_arm64_gnu -d tarballs",
          "install-object CommonsBase_Std.Coreutils.Tar@0.2.2 -s File.Linux_arm64 -d tarballs",
          "install-object CommonsBase_Std.Coreutils.Tar@0.2.2 -s File.Linux_arm32 -d tarballs",
          "install-object CommonsBase_Std.Coreutils.Tar@0.2.2 -s File.Linux_x86_gnu -d tarballs",
          "install-object CommonsBase_Std.Coreutils.Tar@0.2.2 -s File.Linux_x86 -d tarballs",
          "install-object CommonsBase_Std.Coreutils.Tar@0.2.2 -s File.Linux_x86_64 -d tarballs",
          "install-object CommonsBase_Std.Coreutils.Tar@0.2.2 -s File.Linux_x86_64 -d tarballs"
        ]
      },
      "function": {
        "execution": [
          // 7z.exe only runs on Windows
          {
            "name": "OSFamily",
            "value": "windows"
          }
        ],
        "args": [
          // with [7z.exe] ...
          "bin/File.Windows_x86/7z.exe",
          // (e)xtract all files into a single flat directory
          "e",
          // to output directory for slot [File.Darwin_arm64] (etc.)
          "-o${SLOT.request}",
