      },
      "outputs": {
        "files": [
          {
            "paths": ["coreutils", "LICENSE", "README.package.md", "README.md"],
            "slots": ["File.Darwin_arm64", "File.Darwin_x86_64", "File.Linux_arm64_gnu", "File.Linux_arm64", "File.Linux_arm32", "File.Linux_x86_gnu", "File.Linux_x86", "File.Linux_x86_64"]
          }
        ]
      }
    }
  ]
